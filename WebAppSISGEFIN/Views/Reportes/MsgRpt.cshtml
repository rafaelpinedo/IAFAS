@model WebAppSISGEFIN.Models.rMensaje
@{
    Layout = "_LayoutRpt.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="row shadow-sm marco" style="margin-top:50px !important" >
                <div class="col-md-3">
                    <br />
                    <center>
                        <img src="@Model.Image" width="200px" height="225px" />
                    </center>
                </div>
                <div class="col-md-9" style="font-family: Tahoma; font-size: 20px; ">
                    <br />
                    <h3>
                        @Model.Type
                    </h3>
                    @Model.Message
                    <br />
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>
